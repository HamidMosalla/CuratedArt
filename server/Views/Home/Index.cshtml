@using CuratedArt.FrontEndArchitecture
@inject WebpackModules WebpackModules

@section Head {
    @foreach (var style in WebpackModules.GetStyles(Context.Request))
    {
        <link rel="stylesheet" href="@style" />
    }
}

@section Scripts {
     @foreach (var script in WebpackModules.GetScripts(Context.Request))
    {
        <script type="text/javascript" src="@script"></script>
    }
}