@using CuratedArt.FrontEndArchitecture
@inject FrontEndAssetRenderer _frontEndAssetRenderer

@section Head {
    @foreach (var style in _frontEndAssetRenderer.GetStyles(Context.Request))
    {
        <link rel="stylesheet" href="@style" />
    }
}

@section Scripts {
     @foreach (var script in _frontEndAssetRenderer.GetScripts(Context.Request))
    {
        <script type="text/javascript" src="@script"></script>
    }
}